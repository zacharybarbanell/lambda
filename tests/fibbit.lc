SUCC := λn. λf. λx. f (n f x)
PLUS := λm. λn. m SUCC n
ZERO := λf. λx. x
FOUR := SUCC SUCC SUCC SUCC ZERO
TRUE := λt. λe. t
FALSE := λt. λe. e
IF := λc. λt. λe. c t e
AND := λt. λe. t e t
ISZERO := λn. n (λm. FALSE) TRUE
PAIR := λa. λb. λs. s a b
FST := λp. p TRUE
SND := λp. p FALSE
T := λp. PAIR (SND p) (SUCC (SND p))
PRED := λn. FST (n T (PAIR ZERO ZERO))
MINUS := λm. λn. n PRED m
EQUAL := λn. λm. ISZERO (MINUS n m) AND ISZERO (MINUS m n)
FIBITER := λp. PAIR (SND p) (PLUS (FST p) (SND p))
FIBBIT := λn. FST (n FIBITER (PAIR ZERO (SUCC ZERO)))
FIBBIT FOUR
